body
    background #ccc
    overflow-x hidden
    
::-webkit-scrollbar
    width 0px

#wrapper-container
    position relative
    width calc(100% - 200px)
    height 100%
    margin-left 200px
    margin-top 70px
    z-index -1
    #wrapper
        padding 0px 10px
        width 100%

.margin-top
    margin-top 10px
.margin-bottom
    margin-bottom 10px
.margin-left
    margin-left 10px
.margin-right
    margin-right 10px
.margin
    margin 10px

@import "app/components/sidebar/styles/sidebar.styl"
@import "app/components/sidebarItem/styles/sidebarItem.styl"
@import "app/components/navMenu/styles/navMenu.styl"
@import "app/components/navMenuItem/styles/navMenuItem.styl"
@import "app/components/widget/styles/widget.styl"
