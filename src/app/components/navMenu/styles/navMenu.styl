bg-color = #2196F3
table-normal-border = #cccccc
table-backgronud-hover = #dddddd
.nav-menu
    display block
    position fixed
    z-index 9
    top 0
    left 0
    right 0
    width 100%
    height 54px
    background bg-color
    box-shadow 0px 2px 9px #555
    .app-info
        float left
        display inline-block
        height 100%
        width 200px
        .logo
            h4
                margin 0
                text-align center
                color #eee
                line-height 54px
                font-family 'Lato'
    .nav-menu-options
        display inline-block
        float right
        height 100%
        margin 0
        padding 0
